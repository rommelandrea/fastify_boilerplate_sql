  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "npm start",
      "skipFiles": ["<node_internals>/**"],
      "preLaunchTask": "npm: start",
      "program": "${workspaceFolder}/dist/server.js",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "program": "${workspaceFolder}/dist/server.js",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },

    {
      "command": "npm run dev",
      "name": "run dev",
      "request": "launch",
      "type": "node-terminal"
    },

    {
      "command": "npm run test",
      "name": "run test",
      "request": "launch",
      "type": "node-terminal"
    }
  ]
}
